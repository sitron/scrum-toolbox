<div id="sprint-days-container"></div>

<script type="text/x-template" id="sprint-day-template">
    <div>{ date }</div>
    <div class="inline-updatable">
        <span class="">{ nbHours }</span>
        <input class="nbHours" type="text" value="{ nbHours }" />
    </div>
    <div class="inline-updatable">
        <span class="">{ nbSP }</span>
        <input class="nbSP" type="text" value="{ nbSP }" />
    </div>
    <div class="inline-updatable">
        <span class="">{ nbBV }</span>
        <input class="nbBV" type="text" value="{ nbBV }" />
    </div>
</script>

